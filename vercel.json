{
  "version": 2,
  "framework": null,
  "public": "Games",
  "cleanUrls": false,
  "routes": [
    {
      "src": "^/$",
      "dest": "/Zones.json"
    },
    {
      "src": "^/Games/?$",
      "dest": "/Zones.json"
    },
    {
      "src": "^/Games/([^/]+)/?$",
      "dest": "/Zones.json"
    },
    {
      "src": "^/Games/([^/]+)/([^/]+)/?$",
      "dest": "/Zones.json"
    }
  ],
  "ignoreCommand": "echo 'Skip checking everything except Games/Zones.json'"
}
